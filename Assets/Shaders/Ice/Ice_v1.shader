				float3 WorldNormal10 = tangentToWorldDir9;
				float3 RefractDir28 = refract( -ase_worldViewDir , WorldNormal10 , _IOR );
				float2 uv_IceTex = IN.ase_texcoord3.xy * _IceTex_ST.xy + _IceTex_ST.zw;
